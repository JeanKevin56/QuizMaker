# Development Environment Configuration
NODE_ENV=development
VITE_APP_NAME=Quiz Platform (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_API_BASE_URL=http://localhost:3000
VITE_ENABLE_DEBUG=true
VITE_ENABLE_CONSOLE_LOGS=true
VITE_GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent
VITE_PDF_WORKER_SRC=/node_modules/pdfjs-dist/build/pdf.worker.js
VITE_STORAGE_PREFIX=quiz_platform_dev_
VITE_MAX_FILE_SIZE=10485760
VITE_SUPPORTED_FILE_TYPES=application/pdf,text/plain
VITE_DEFAULT_QUIZ_SETTINGS={"shuffleQuestions":false,"showExplanations":true,"timeLimit":null}